<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" ToolsVersion="Current" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ItemGroup Label="ProjectConfigurations">
    <ProjectConfiguration Include="Debug|x64">
      <Configuration>Debug</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Hybrid|x64">
      <Configuration>Hybrid</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
    <ProjectConfiguration Include="Release|x64">
      <Configuration>Release</Configuration>
      <Platform>x64</Platform>
    </ProjectConfiguration>
  </ItemGroup>
  <PropertyGroup Label="Globals">
    <ProjectGuid>{FA3B75F2-51A6-4D7C-BC48-149375905ED4}</ProjectGuid>
    <Keyword>MFCProj</Keyword>
    <MaxSDK>../../../../../maxsdk</MaxSDK>
  </PropertyGroup>
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.Default.props" />
  <Import Project="$(MaxSDK)\ProjectSettings\PropertySheets\3dsmax.general.project.settings.props" />
  <PropertyGroup>
    <ConfigurationType>Application</ConfigurationType>
    <UseOfMfc>Dynamic</UseOfMfc>
  </PropertyGroup>
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.props" />
  <ImportGroup Label="PropertySheets">
    <Import Project="$(MaxSDK)\ProjectSettings\PropertySheets\3dsmax.cpp.props" />
  </ImportGroup>
  <PropertyGroup>
    <_ProjectFileVersion>10.0.30319.1</_ProjectFileVersion>
    <comsrv_intermediates>$(MaxDevelDir)/3dswin/obj/$(Platform)/$(Configuration)/comsrvgup</comsrv_intermediates>
    <comsrv_intermediates Condition="'$(IsMaxSdkProject)'=='true' AND '$(IsMaxProjectInDevTree)'=='false'">$(MaxSdkDir)/obj/$(Platform)/$(Configuration)/comsrvgup</comsrv_intermediates>
    <OutDir>$(MaxSdkPlugin)</OutDir>
    <TargetName>MaxClient</TargetName>
    <TargetExt>.exe</TargetExt>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(MaxSdkInc);$(comsrv_intermediates);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <ForcedIncludeFiles>maxclient_pch.h</ForcedIncludeFiles>
      <PrecompiledHeader>Use</PrecompiledHeader>
      <PrecompiledHeaderFile>maxclient_pch.h</PrecompiledHeaderFile>
      <PreprocessorDefinitions>_CRT_NON_CONFORMING_SWPRINTFS;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <ResourceCompile>
      <AdditionalIncludeDirectories>$(MaxSdkInc);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ResourceCompile>
  </ItemDefinitionGroup>
  <ItemGroup>
    <ClCompile Include="MaxClient.cpp" />
    <ClCompile Include="MaxClientDlg.cpp" />
    <ClCompile Include="maxclient_pch.cpp">
      <PrecompiledHeader>Create</PrecompiledHeader>
    </ClCompile>
  </ItemGroup>
  <ItemGroup>
    <ResourceCompile Include="MaxClient.rc" />
  </ItemGroup>
  <ItemGroup>
    <ClInclude Include="MaxClient.h" />
    <ClInclude Include="MaxClientDlg.h" />
    <ClInclude Include="maxclient_pch.h" />
    <ClInclude Include="Resource.h" />
  </ItemGroup>
  <ItemGroup>
    <None Include="res\MaxClient.ico" />
    <None Include="res\preview.bmp" />
    <None Include="res\comsrv.tlb" />
  </ItemGroup>
  <ItemGroup>
    <!-- comsrvgup is added as project reference because maxclient depends on comsrv.tlb.
    This ensures that the build system knows there is a dependency.
    See #import directive in maxclient_pch.h. -->
    <ProjectReference Include="..\comsrvgup.vcxproj">
      <Project>{d6a991fa-cea4-4f17-bef0-1cedf83ce58f}</Project>
    </ProjectReference>
  </ItemGroup>
  <Import Project="$(VCTargetsPath)\Microsoft.Cpp.targets" />
</Project>