<?xml version="1.0" encoding="utf-8"?>
<Project DefaultTargets="Build" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <ImportGroup Label="PropertySheets">
  </ImportGroup>
  <PropertyGroup Label="UserMacros" />
  <PropertyGroup>
    <NiflibRoot Condition="'$(NiflibRoot)'=='' and Exists('$(ProjectDir)..\..\niflib\include\niflib.h')">$(ProjectDir)..\..\niflib</NiflibRoot>
    <NiflibRoot Condition="'$(NiflibRoot)'=='' and Exists('C:\Users\mello\Desktop\xaxSource\niflib\include\niflib.h')">C:\Users\mello\Desktop\xaxSource\niflib</NiflibRoot>
    <NiflibRoot Condition="'$(NiflibRoot)'==''">C:\src\niflib</NiflibRoot>
  </PropertyGroup>
  <PropertyGroup>
    <_ProjectFileVersion>14.0.23107.0</_ProjectFileVersion>
  </PropertyGroup>
  <ItemDefinitionGroup>
    <ClCompile>
      <AdditionalIncludeDirectories>$(NiflibRoot)\include;$(NiflibRoot);%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
      <PreprocessorDefinitions>USE_NIFLIB_TEMPLATE_HELPERS;NIFLIB_STATIC_LINK;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <AdditionalLibraryDirectories>$(NiflibRoot)\build_vs2022_x64\Release;$(NiflibRoot)\lib;%(AdditionalLibraryDirectories)</AdditionalLibraryDirectories>
      <AdditionalDependencies>niflib_static.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemGroup />
</Project>